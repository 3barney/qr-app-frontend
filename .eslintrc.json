{
  "parser": "babel-eslint",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "parserOptions": {
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    }
  },
  "extends" : [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb-base"
  ],
  "rules" : {
    "arrow-body-style":"warn",
    "no-underscore-dangle":"off",
    "no-undef":"off",
    "no-unused-vars":"off",
    "class-methods-use-this":"off",
    "max-len":"off",
    "arrow-parens":"off"
  }
}
